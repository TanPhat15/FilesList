{
  "name": "file-viewer",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "build": "ng build",
    "electron:serve": "ng build && electron .",
    "start": "npm run setup && concurrently \"npm run ng:serve\"",
    "electron": "electron .",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:file-viewer": "node dist/file-viewer/server/server.mjs",
    "setup": "npm install && npm install primeicons primeng @primeng/themes primeflex concurrently"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@primeng/themes": "^20.1.2",
    "express": "^4.21.2",
    "ngx-pagination": "^6.0.3",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.16",
    "@angular/cli": "^19.2.16",
    "@angular/compiler-cli": "^19.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.0",
    "electron-reload": "^2.0.0-alpha.1",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "wait-on": "^8.0.4"
  }
}
